define(["jquery","common/essentialData"],function($,data){var $login=$("#login a");var $logout=$("#logout a");var loginUrlHandler={init:function(){this.appendReturnUrl()},getEncodedCurrentURL:function(){var url=document.URL;return encodeURIComponent(encodeURIComponent(url))},appendReturnUrl:function(){var returnUri=this.getEncodedCurrentURL();$login.attr("href",$login.attr("href")+returnUri);$logout.attr("href",$logout.attr("href")+returnUri)},isLogin:function(){return data.getLogin()==="Y"},getLoginUrl:function(){return $login.attr("href")},
getLogoutUrl:function(){return $logout.attr("href")},isLoginToLoginPage:function(){if(!this.isLogin()){location.href=this.getLoginUrl();return true}return false}};return loginUrlHandler});