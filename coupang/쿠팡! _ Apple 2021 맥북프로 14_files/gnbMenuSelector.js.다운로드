define(["jquery","common/utils/validationUtils","common/menuSelector","common/essentialData"],function($,validator,MenuSelector,essentialData){var AllCategoryView=function(){var caller=$(".category-all"),close=$("#gnb .category-list").find(".close"),catelist=$("#gnb .category-list");caller.click(function(e){e.preventDefault();$(this).toggleClass("show");catelist.toggle()});close.click(function(e){e.preventDefault();caller.removeClass("show");catelist.hide()})};new AllCategoryView;var standbyEventHandler=
function(e){};var clearEventHandler=function(e){};var activeEventHandler=function(e,activeTarget){if(essentialData.getAbTestOptionByKey("1957")==="B"){var target=$(activeTarget),targetSubDepth=target.find(".second-depth-list:first-child");$(".category-layer li").filter(".hover").removeClass("hover");target.addClass("hover");targetSubDepth.addClass("hover");target.parent().parent().parent().addClass("category-2depth-active")}else{$(".category-layer \x3e .menu \x3e li").filter(".hover").removeClass("hover");
$(activeTarget).addClass("hover");$(activeTarget).parent().parent().parent().addClass("category-2depth-active")}};var deactiveEventHandler=function(e,activeTarget){if(essentialData.getAbTestOptionByKey("1957")==="B"){$(".category-layer li").filter(".hover").removeClass("hover");$(activeTarget).parent().parent().parent().removeClass("category-2depth-active")}else{$(".category-layer \x3e .menu \x3e li").filter(".hover").removeClass("hover");$(activeTarget).parent().parent().parent().removeClass("category-2depth-active")}};
new MenuSelector({menuPanelSelector:".category-layer",menuRowSelector:".menu \x3e li",targetSelector:".category-layer li.hover .depth-list",delayTime:200,tolerance:40,standbyHandler:standbyEventHandler,activeHandler:activeEventHandler,deactiveHandler:deactiveEventHandler,clearHandler:clearEventHandler});var thirdDepthStandbyEventHandler=function(e){};var thirdDepthClearEventHandler=function(e){};var thirdDepthActiveEventHandler=function(e,activeTarget){$(".second-depth-list").filter(".hover").removeClass("hover");
$(activeTarget).addClass("hover")};var thirdDepthDeactiveEventHandler=function(e,activeTarget){$(activeTarget).removeClass("hover")};new MenuSelector({menuPanelSelector:".depth-list.third \x3e ul",menuRowSelector:".second-depth-list",targetSelector:".depth-list.third .third-depth-list",delayTime:200,tolerance:40,standbyHandler:thirdDepthStandbyEventHandler,activeHandler:thirdDepthActiveEventHandler,deactiveHandler:thirdDepthDeactiveEventHandler,clearHandler:thirdDepthClearEventHandler});var travelThirdDepthStandbyEventHandler=
function(e){};var travelThirdDepthClearEventHandler=function(e){};var travelThirdDepthActiveEventHandler=function(e,activeTarget){$(".travel-second-depth-row").filter("hover").removeClass("hover");$(activeTarget).addClass("hover")};var travelThirdDepthDeactiveEventHandler=function(e,activeTarget){$(activeTarget).removeClass("hover")};new MenuSelector({menuPanelSelector:".travel-category-nav .travel-second-depth-list",menuRowSelector:".travel-second-depth-row",targetSelector:".travel-category-nav .travel-third-depth-list",
delayTime:200,tolerance:40,standbyHandler:travelThirdDepthStandbyEventHandler,activeHandler:travelThirdDepthActiveEventHandler,deactiveHandler:travelThirdDepthDeactiveEventHandler,clearHandler:travelThirdDepthClearEventHandler})});