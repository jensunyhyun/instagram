define(["jquery"],function($){var utility={uiEnhancements:function(){if(!this.uiString)this.uiString=this.ui;for(var key in this.uiString)this.ui[key]=typeof this.uiString[key]==="function"?this.uiString[key]():$(this.uiString[key])},price:{addComma:function(number){var reg=/(^[+-]?\d+)(\d{3})/;if(isNaN(number))return 0;number+="";while(reg.test(number))number=number.replace(reg,"$1"+","+"$2");return number},removeComma:function(string){var value="";string=string.replace(/,/g,"");isNaN(string)?value=
0:value=Number(string);return value}},date:{splitDate:function(str){str=str.toString();if(str.length!==8)return{year:"00000",month:"00",day:"00"};var date=str.match(/(\d{4})(\d{2})(\d{2})/);return{year:date[1],month:date[2],day:date[3]}},yyyymmdd_dot:function(str){var object=utility.date.splitDate(str);return object.year+"."+object.month+"."+object.day}},getAndroidVersion:function(ua){ua=(ua||navigator.userAgent).toLowerCase();var match=ua.match(/android\s([0-9\.]*)/);return match?match[1]:false}};
return utility});