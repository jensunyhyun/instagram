define(["jquery"],function($){var browserChecker={isOpera:function(){var isOpera=!!window.opera||navigator.userAgent.indexOf(" OPR/")>=0;return isOpera},isFirefox:function(){var isFirefox=typeof InstallTrigger!=="undefined";return isFirefox},isSafari:function(){var isSafari=Object.prototype.toString.call(window.HTMLElement).indexOf("Constructor")>0;return isSafari},isChrome:function(){var isChrome=!!window.chrome&&!this.isOpera();return isChrome},isIE:function(){var isIE=false||!!document.documentMode;
return isIE},clientWidth:function(){var clientWidth=$(window).width();return clientWidth},clientHeight:function(){var clientHeight=$(window).height();return clientHeight},isMobile:function(){var isMobile=navigator.userAgent.match(/(Android|webOS|iPad|iPhone|iPod)/i);return isMobile}};return browserChecker});