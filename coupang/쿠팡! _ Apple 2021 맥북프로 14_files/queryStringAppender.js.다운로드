define(["jquery","common/utils/validationUtils"],function($,validator){var queryStringAppender={isAlreadyReplaced:function($element,dataKey){return validator.isNotEmpty($element.data(dataKey))},insertReplacedFlag:function($element,dataKey){$element.data(dataKey,true)},getUrlWithQueryStringDelimiter:function(url,queryString){if(validator.isEmpty(url)||url.indexOf("javascript")==0)return url;var delimiter="?";if(url.indexOf(delimiter)>=0)delimiter="\x26";return url+delimiter+queryString}};return queryStringAppender});