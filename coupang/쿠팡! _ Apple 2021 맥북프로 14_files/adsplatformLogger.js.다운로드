define(["jquery"],function($){var adsplatformLogger=function(indicator,clickLogOnly){this.init(indicator,clickLogOnly)};adsplatformLogger.prototype={init:function(indicator,clickLogOnly){var local=this;$(indicator).find("a").each(function(){var data=$(this).data("adsplatform");if(data){if(!clickLogOnly)local.logForImpression(data,$(this));$(this).bind("click",function(){local.logForClick(data,$(this))})}})},logForClick:function(data,indicator){if(data.clickLogUri)this.execLogging(data.clickLogUri,
indicator)},logForImpression:function(data,indicator){if(data.impressionLogUri)this.execLogging(data.impressionLogUri,indicator)},logForVeiwableImpression:function(data){},execLogging:function(url,indicator){if(!$.support.cors&&window.XDomainRequest)setTimeout(function(){indicator.prepend('\x3cimg style\x3d"display: none" src\x3d"'+url+'" /\x3e')},1E3);else $.ajax({type:"GET",url:url})}};return adsplatformLogger});