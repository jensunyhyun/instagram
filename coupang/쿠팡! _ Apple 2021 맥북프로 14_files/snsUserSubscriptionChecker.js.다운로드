define(["jquery","common/template/urlBuilder","common/template/snsNotificationCount"],function($,urlBuilder,snsNotificationCount){var console=window.console||{error:function(){}};var snsUserSubscriptionChecker={init:function(){this.getSubscriptionCountByUser()},getSubscriptionCountByUser:function(){$.ajax({url:urlBuilder.buildRequestUrl("/np/sns/user/subscription/count"),type:"GET",dataType:"jsonp",jsonp:"callback",success:function(result){if(result){var count=result.subscriptionCount;if(count>0){snsNotificationCount.init();
$(".subscription-menu-a").css("display","block")}}},error:function(request,statue,error){console.error("snsUserSubscriptionChecker ajax error!")}})}};return snsUserSubscriptionChecker});