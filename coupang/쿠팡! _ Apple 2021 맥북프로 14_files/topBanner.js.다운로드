define(["jquery","common/utils/cookieUtils"],function($,cookie){var COUPANG_BANNER_ID="#coupang-banner",COOKIE_KEY="EVT_FDC_NOTI";if(cookie.getValue(COOKIE_KEY)==="N")return;var $wrapper=$(COUPANG_BANNER_ID),$topBannerAnchor=$wrapper.find("span").find("a"),href=$topBannerAnchor.attr("href"),$bigTopBannerImgWrap=$wrapper.find("strong"),$bigTopBannerImg=$bigTopBannerImgWrap.find("img");if(!href&&$bigTopBannerImg.length>0){$bigTopBannerImg.attr("src",$bigTopBannerImg.data("bigimg"));$topBannerAnchor.click(function(evt){toggleBigBanner(evt)});
$bigTopBannerImgWrap.click(function(evt){toggleBigBanner(evt)})}function toggleBigBanner(evt){evt.preventDefault();$bigTopBannerImgWrap.stop().slideToggle("slow")}coupangWebRequire(["common/ToggleSlider"],function(ToggleSlider){(new ToggleSlider({"wrapper":COUPANG_BANNER_ID,"showBtn":"","closeBtn":COUPANG_BANNER_ID+" a.close"})).init().setCookieData({"key":COOKIE_KEY,"value":"N","expiryTime":{timeUnit:"DAY",timeValue:0}})})});