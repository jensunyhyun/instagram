define(["jquery"],function($){var validationUtil={isEmpty:function(arg){if(arg===undefined||arg===null||arg==="null"||$.trim(arg)===""||arg.length===0||typeof arg==="object"&&$.isEmptyObject(arg))return true;return false},isNotEmpty:function(arg){return!this.isEmpty(arg)},defaultValue:function(arg,defaultValue){if(this.isEmpty(arg))return defaultValue;return arg},isFunction:function(fn){return typeof fn==="function"},isEmailFormat:function(arg){var format=/^[_0-9a-zA-Z-]+(\.[_0-9a-zA-Z-]+)*@[0-9a-zA-Z-]+(\.[0-9a-zA-Z-]+)*$/;
if(!format.test(arg))return false;else return true},isEmptyByHref:function(arg){if(this.isEmpty(arg))return true;arg=$.trim(arg);if(arg.indexOf("#")==0||arg.indexOf("javascript")>=0)return true;return false}};return validationUtil});